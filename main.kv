MDScreen:
    MDBoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title:'Alarm App'
            elevation: 20
            left_action_items:[['menu',lambda x: app.navigation_draw()]]
            right_action_items:[['alarm',lambda x: app.navigation_draw()]]
        MDLabel:
            text: "text"
            color:[1,0,1,1]
            halign:'center'
        MDBottomAppBar:
            MDToolbar:
                icon:'bell'
                mode:'end'
                type:'bottom'
                on_action_button:app.navigation_draw()
                left_action_items:[['fire',lambda x: app.navigation_draw()]]
                
                #right_action_items:[['clock',lambda x: app.navigation_draw()]]
